local Luna = loadstring(game:HttpGet("https://raw.githubusercontent.com/Nebula-Softworks/Luna-Interface-Suite/refs/heads/master/source.lua"))()

local Window = Luna:CreateWindow({
    Name = "Legitxwx Hub | v1.1.0",
    Subtitle = "Plants vs Brainrots",
    LogoID = "82795327169782",
    LoadingEnabled = true,
    LoadingTitle = "Legitxwx Hub",
    LoadingSubtitle = "by xwx",
    ConfigSettings = {ConfigFolder = "LegitxwxHub"},
    KeySystem = false
})

-- Now create other tabs
local gearTab = Window:CreateTab({Name = "üåü Gears", Icon = "tool"})
local seedTab = Window:CreateTab({Name = "üå± Seeds", Icon = "leaf"})
local brainTab = Window:CreateTab({Name = "üß† Brainrots", Icon = "brain"})
local localTab = Window:CreateTab({Name = "üßç Local", Icon = "user"})
local updTab   = Window:CreateTab({Name = "UPD LOG", Icon = "scroll"})

-- Example: add a paragraph to test
gearTab:CreateParagraph({Title = "Test Tab", Text = "If you see this, tabs are working!"})
seedTab:CreateParagraph({Title = "Test Tab", Text = "Seed Tab works!"})
brainTab:CreateParagraph({Title = "Test Tab", Text = "Brainrots Tab works!"})
localTab:CreateParagraph({Title = "Test Tab", Text = "Local Tab works!"})
updTab:CreateParagraph({Title = "Test Tab", Text = "Update Log Tab works!"})
